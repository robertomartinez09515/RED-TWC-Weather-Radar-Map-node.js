{
    "name": "Node-RED-TWC-Weather-Radar-Map",
    "description": "Node-RED Dashboard which displays a time-lapse North America Satellite / Radar Weather map using the TWC weather apis",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/johnwalicki/Node-RED-TWC-Weather-Radar-Map.git"
    },
    "keywords": [
        "node-red",
        "weather",
        "twc",
        "forecast",
        "radar"
    ],
    "author": "John Walicki <johnwalicki@gmail.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "node-red": "2.x",
        "node-red-contrib-image-tools": "1.x",
        "node-red-dashboard": "2.x",
        "node-red-node-base64": "0.x",
        "node-red-contrib-twc-weather": "0.x",
	"node-red-contrib-web-worldmap":"2.x",
        "node-red-contrib-ui-multistate-switch": "1.x"
    },
    "node-red": {
        "settings": {
            "flowFile": "radarmap-flows.json",
            "credentialsFile": "radarmap-flows_cred.json"
        }
    }
}
